<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.han.mapper.MyUserMapper">
    <resultMap id="BaseResultMap" type="com.han.model.MyUser">
        <!--property对应model字段，column对应表中字段-->
        <id property="id" column="id"/>
        <result property="username" column="username"/>
        <result property="password" column="password"/>
        <result property="token" column="token"/>
    </resultMap>

    <select id="listAllMyUsers" resultMap="BaseResultMap">
        select *
        from t_myuser
    </select>
    <select id="getMyUserById" resultMap="BaseResultMap">
        select *
        from t_myuser
        where id = #{id}
    </select>
</mapper>
